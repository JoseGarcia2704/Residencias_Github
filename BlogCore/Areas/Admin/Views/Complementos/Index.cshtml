<div class="container mt-4">
    <div class="row">
        <div class="col-8">
            <h2 class="text-primary">Lista de Complementos</h2>
        </div>
        <div class="col-4" text-right>
            <a asp-controller="Proveedores" asp-action="Create" class="btn btn-outline-primary">
                <i class="fas fa-plus" style="margin-right: 5px;"></i>
                Crear nuevo complemento
            </a>
        </div>
    </div>
    <br />
    <br />
    <table id="tblComplementos" class="table table-striped table table-bordered" style="width:100%">
        <thead>
            <tr class="table-primary">
                <th>UUIDC</th>
                <th>Monto</th>
                <th>saldoInsoluto</th>
                <th>Acciones</th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts{
    <script src="~/js/complementos.js"></script>
}
